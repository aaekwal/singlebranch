pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo "building the application"
        }
       }
    stage('Test') {
      steps {
        echo "testing the application"
        }
       }
    stage('Deploy') {
      steps {
        echo "deploying the application"
        }
       }
    stage('Release') {
      steps {
        echo "releasing application to staging"
        }
       }
    stage('Production') {
      steps {
        echo "release to Production"
      }
    }
   }
  }
